  "scripts": {
    "test": "jest",
    "test-run": "ts-node src/test-run.ts",
    "start": "ts-node src/index.ts",
    "start:watch": "nodemon ./src/index.ts",
    "start:debug": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:debug:watch": "nodemon --inspect ./src/index.ts",
    "cli": "ts-node ./src/cli.ts",
    "cli:debug": "node --inspect=5859 -r ts-node/register ./src/cli.ts",
    "cli:debug:watch": "nodemon --inspect=5859 ./src/cli.ts",
    "play": "ts-node src/playground/playground.ts",
    "play:debug": "node --inspect=5860 -r ts-node/register ./src/playground/playground.ts",
    "play:debug:watch": "nodemon --inspect=5860 ./src/playground/playground.ts",
    "generate-route-types": "ts-node src/generate-route-types.ts",
    "generate-route-types:debug": "node --inspect=5860 -r ts-node/register ./src/generate-route-types.ts",
    "generate-route-types:debug:watch": "nodemon --inspect=5860 ./src/generate-route-types.ts",
    "build": "npm run clean && tsc",
    "build:run": "npm run build && npm run run:build",
    "run:build": "node ts-built/index.js",
    "run:built:test-run": "node ts-built/test-run.js",
    "sequelize": "npm run build && sequelize",
    "sequelize:migrations": "npm run sequelize -- db:migrate",
    "sequelize:migrations:devundo": "npm run sequelize -- db:migrate:undo:all --to 20201112094400-curriculum-assessments.js",
    "sequelize:built:migrations": "sequelize -- db:migrate",
    "lint": "eslint src/ --ext .ts",
    "clean": "if [ -d ts-built ]; then rm -r ts-built; fi;",
    "package": "node ./package.js",
    "build:package": "npm run build && npm run package"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts",
    "execMap": {
      "ts": "node --require ts-node/register"
    },
    "signal": "SIGINT"
